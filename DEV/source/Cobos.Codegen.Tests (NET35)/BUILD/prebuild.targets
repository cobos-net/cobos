  <!-- Database properties -->
  <PropertyGroup>
    <DatabaseToXsdConnectionString>Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=vea795db1.ingrnet.com)(PORT= 1521))(CONNECT_DATA=(SERVER=DEDICATED)(SERVICE_NAME=ORC102.VEA795DB1)));User Id=eadev;Password=eadev</DatabaseToXsdConnectionString>
    <DatabaseToXsdDatabasePlatform>Oracle</DatabaseToXsdDatabasePlatform>
    <DatabaseToXsdDatabaseSchema>EADEV</DatabaseToXsdDatabaseSchema>
  </PropertyGroup>

  <ItemGroup>
    <DatabaseToXsdTable Include="AEVEN;EVENT;EVCOM;CD_UNITS;UN_HI;DISPASS_EVENT;EV_DISPO;DIVERT;INCIDENT_TRACKING;VEHIC;PERSO;TOW_VEHIC;CONT_NAME;PROPT"/>
