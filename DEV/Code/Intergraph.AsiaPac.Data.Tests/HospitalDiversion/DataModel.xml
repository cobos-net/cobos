<?xml version="1.0" encoding="utf-8" ?>
<DataModel name="HewsDataModel" xmlns="http://schemas.intergraph.com/asiapac/cad/datamodel/1.0.0" xmlns:cad="http://schemas.intergraph.com/asiapac/cad/datamodel/1.0.0">

	<!--
	~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	Hospital Diversion message
	~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	-->

	<Object name="HospitalDiversionMessage" dbTable="DIVERT">
		<XsdProperty name="Action" type="HosptialDiversionActionEnum" minOccurs="1"/>
		<Property name="Open" dbColumn="open_record" stringFormat="CadBoolean"/>
		<Property name="Location" dbColumn="location"/>
		<Property name="Type" dbColumn="d_type" dbAlias="d_type_type" stringFormat="Separator" formatArgs="| 0"/>
		<Property name="SubType" dbColumn="d_type" dbAlias="d_type_subtype" stringFormat="Separator" formatArgs="| 1"/>
		<Property name="ExpiryTimestamp" dbColumn="expiry_dts" stringFormat="CadDts"/>
		<Property name="Comment" dbColumn="d_comm"/>
		<Property name="ItemNumber" dbColumn="item_number"/>
		<Property name="Notifications" dbColumn="notifications"/>
		<Property name="CreatedTerminal" dbColumn="cterm"/>
		<Property name="ClosedBy" dbColumn="x_caller"/>
		<Property name="ClosedComments" dbColumn="x_comm"/>
		<Property name="ClosedTerminal" dbColumn="xterm"/>
		<Property name="ClosedTimestamp" dbColumn="xdts" stringFormat="CadDts"/>
	</Object>

	<Enumeration name="HosptialDiversionActionEnum" base="xsd:string">
		<Item>ADD</Item>
		<Item>REMOVE</Item>
	</Enumeration>
	
</DataModel>